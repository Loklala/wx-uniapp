<view class="content"><view class="mainUI"><view class="title" style="{{'padding-top:'+(titleheight+'px')+';'}}"><text>RTMP推流</text></view><view class="item" style="padding-top:2vh;"><text class="item-title">推流获取</text><button data-event-opts="{{[['tap',[['onScanQR']]]]}}" class="item-button1" bindtap="__e">扫码读取</button><button data-event-opts="{{[['tap',[['onNewUrlClick']]]]}}" class="item-button1" bindtap="__e">自动生成</button></view><view class="item"><text class="item-title">推流地址</text><input class="item-input" placeholder="rtmp://" data-event-opts="{{[['focus',[['focusPush']]],['tap',[['onPushInputTap']]],['input',[['onPushInputChange']]]]}}" value="{{pushUrl}}" bindfocus="__e" bindtap="__e" bindinput="__e"/><image class="item-image" src="../../../static/images/copy.png" data-event-opts="{{[['tap',[['onCopyPushUrl',['$event']]]]]}}" bindtap="__e"></image></view><view class="item"><text class="item-title">播放地址</text><input class="item-input" placeholder="http://" data-event-opts="{{[['focus',[['focusPlay']]],['tap',[['onPlayInputTap']]],['input',[['onPlayInputChange']]]]}}" value="{{playUrl}}" bindfocus="__e" bindtap="__e" bindinput="__e"/><image class="item-image" src="../../../static/images/copy.png" data-event-opts="{{[['tap',[['onCopyPlayUrl']]]]}}" bindtap="__e"></image></view><view class="item"><text class="item-title">画面质量</text><radio-group data-event-opts="{{[['change',[['modeRadioChange',['$event']]]]]}}" class="radio-group" bindchange="__e"><block wx:for="{{modeItems}}" wx:for-item="item" wx:for-index="index"><label class="{{['radio',mode==item.value?'bc_blue white':'blue']}}"><radio value="{{item.value}}" checked="{{item.checked}}"></radio>{{''+item.title+''}}</label></block></radio-group></view><view class="item"><text class="item-title">画面方向</text><radio-group data-event-opts="{{[['change',[['orientationRadioChange',['$event']]]]]}}" class="radio-group" bindchange="__e"><block wx:for="{{orientationItems}}" wx:for-item="item" wx:for-index="index"><label class="{{['radio',orientation==item.value?'bc_blue white':'blue']}}"><radio value="{{item.value}}" checked="{{item.checked}}"></radio>{{''+item.title+''}}</label></block></radio-group></view><view class="item"><text class="item-title">仅推声音</text><switch class="item-switch" checked="{{!enableCamera}}" data-event-opts="{{[['change',[['switchChange',[this]]]]]}}" bindchange="__e"></switch></view></view><view class="start-box"><button class="start" src="../../../static/images/logo.png" data-event-opts="{{[['tap',[['startPush']]]]}}" bindtap="__e">开始</button></view><view class="logo-box"><image class="logo" src="../../../static/images/logo.png"></image></view><cover-image class="close" style="{{'top:'+(headerHeight+statusBarHeight-26+'rpx')+';'}}" src="../../../static/images/back.png" data-event-opts="{{[['tap',[['onBack']]]]}}" bindtap="__e"></cover-image></view>